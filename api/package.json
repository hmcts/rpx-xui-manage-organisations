{
  "name": "jui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "NODE_ENV=production webpack --config ./webpack-node.config.js",
    "coverage": "cross-env TS_NODE_FILES=true npx nyc mocha -r dotenv-extended/config",
    "watch": "nodemon --watch '**/*.ts' --exec ts-node -r dotenv-extended/config --files local.ts",
    "test": "cross-env TS_NODE_FILES=true mocha -r dotenv-extended/config",
    "test:watch": "cross-env TS_NODE_FILES=true mocha -r dotenv-extended/config --watch --recursive --watch-extensions ts"
  },
  "private": true,
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.js",
      "**/*.d.ts",
      "**/*.spec.ts",
      "application.ts",
      "local.ts",
      "openRoutes.ts",
      "routes.ts",
      "server.ts",
      "webpack-node.config.js"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "require": [],
    "cache": true,
    "all": true,
    "report-dir": "../reports/tests/coverage/node"
  },
  "dependencies": {
    "git-rev-sync": "^3.0.1"
  }
}
