{
  "name": "jui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "tsc",
    "coverage": "npx nyc mocha",
    "watch": "nodemon --watch '**/*.ts' --exec ts-node local.ts",
    "test": "mocha",
    "test:watch": "mocha --watch --recursive --watch-extensions ts"
  },
  "private": true,
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "require": [],
    "cache": true,
    "all": true,
    "report-dir": "../reports/tests/coverage/node"
  }
}
