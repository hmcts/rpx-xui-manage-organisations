    "outDir": "../dist/rpa-pui-manager/api",
    "target": "es5",
    "types": [
      "node",
      "mocha",
      "rxjs"
    ],
    "typeRoots": [
      "./customTypings",
      "node_modules/@types"
    ],
    "baseUrl": ".",
