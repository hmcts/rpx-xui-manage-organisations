{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "no-string-literal": false,
      "object-literal-key-quotes": false,
      "object-literal-sort-keys": false,
      "arrow-parens": [
          true,
          "ban-single-arg-parens"
      ],
      "comment-format": false,
      "indent": [
          true,
          "spaces"
      ],
      "interface-name": [
          true,
          "never-prefix"
      ],
      "max-classes-per-file": false,
      "max-line-length": [
          false,
          3000
      ],
      "member-access": false,
      "no-console": false,
      "no-shadowed-variable": [
          true,
          {
              "import": false
          }
      ],
      "no-unused-variable": [
          true,
          {
              "check-parameters": false
          }
      ],
      "prefer-const": [
          true,
          {
              "destructuring": "all"
          }
      ],
      "quotemark": false,
      "ordered-imports": false,
      "semicolon": false,
      "trailing-comma": false
  },
  "rulesDirectory": []
}
