{
  "name": "rpa-pui-manager",
  "version": "0.0.0",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "start": "node ./dist/rpa-pui-manager/api/server.bundle.js",
    "build": "yarn build:prod && yarn build:node",
    "testx": "cd api && yarn coverage && cd .. && yarn test:ng //node tests are broken need to fix",
    "test": "yarn test:ng",
    "build:prod": "npx ng build --prod",
    "build:node": "cd api && yarn build",
    "start:node": "cd api && yarn watch",
    "start:node:docker": "PUI_ENV=ldocker;cd api && yarn watch",
    "test:node": "cd api && yarn test",
    "test:smoke": "sh whitelist.sh && webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/smoke.conf.js ",
    "test:smoke:local": "sh whitelist.sh && webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/smoke.conf.js --local",
    "test:functional": "sh whitelist.sh && webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/functional.conf.js",
    "test:functional:backup": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/functional.conf.js",
    "test:functional:local": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/functional.conf.js --local",
    "test:fullfunctional": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/fullfunctional.conf.js",
    "test:fullfunctional:local": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/fullfunctional.conf.js --local",
    "test:crossbrowser": "webdriver-manager update && protractor ./test/e2e/config/crossbrowser.conf.js",
    "test:coverage": "echo 'not implemented'",
    "test:node:watch": "cd api && yarn test:watch",
    "test:api": "ts-node ./test/integration/tests/test.ts",
    "test:coverage:ng": "ng test rpa-pui-manager --code-coverage --watch",
    "test:coverage:node": "cd api && yarn coverage",
    "test:mutation": "cd api && yarn test:mutation",
    "ng": "ng",
    "start:ng": "ng serve --port 3000 --host 0.0.0.0 --proxy-config proxy.config.json",
    "build:ng": "ng build",
    "test:a11y": "echo 'ally tests will be executed by test:a11y1 in nightly as its more appropriate for this application and not as part of build. Contact QA for any questions.'",
    "test:a11y1": "mocha --reporter test/accessibility/reporter/customReporter.js  ./test/accessibility/tests/*.js",
    "test:ng": "ng test",
    "lint": "yarn lint:ng",
    "lint:ng": "ng lint",
    "e2e": "ng e2e",
    "pre-push": "ng lint rpa-pui-manager && ng build --prod && ng test rpa-pui-manager --code-coverage",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "rest-assured": "rest-assured",
    "test-pact": "NODE_PATH=. LOG_LEVEL=error mocha --timeout 10000 -r ts-node/register api/test/pact/pact-tests/*.ts",
    "publish-pact": "NODE_PATH=. LOG_LEVEL=error mocha 'api/test/pact/publish/*.js' --timeout 15000 --reporter spec",
    "test:pact:run-and-publish": "yarn test-pact && yarn publish-pact"
  },
